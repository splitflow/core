{
    "name": "@splitflow/core",
    "version": "1.3.3",
    "description": "Splitflow core package",
    "author": "team@splitflow.io",
    "homepage": "https://github.com/splitflow/core#readme",
    "bugs": {
        "url": "https://github.com/splitflow/core/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/splitflow/core.git"
    },
    "type": "module",
    "files": [
        "dist/**/*.js",
        "dist/**/*.cjs",
        "dist/**/*.d.ts"
    ],
    "exports": {
        "./ast": {
            "import": "./dist/ast/index.js",
            "require": "./dist/ast/index.cjs"
        },
        "./style": {
            "import": "./dist/style/index.js",
            "require": "./dist/style/index.cjs"
        },
        "./theme": {
            "import": "./dist/theme/index.js",
            "require": "./dist/theme/index.cjs"
        },
        "./utils/object": {
            "import": "./dist/utils/object.js",
            "require": "./dist/utils/object.cjs"
        },
        "./utils/collection": {
            "import": "./dist/utils/collection.js",
            "require": "./dist/utils/collection.cjs"
        },
        "./utils/dom": {
            "import": "./dist/utils/dom.js",
            "require": "./dist/utils/dom.cjs"
        },
        "./utils/path": {
            "import": "./dist/utils/path.js",
            "require": "./dist/utils/path.cjs"
        },
        "./utils/number": {
            "import": "./dist/utils/number.js",
            "require": "./dist/utils/number.cjs"
        }
    },
    "typesVersions": {
        "*": {
            "ast": [
                "dist/ast/index.d.ts"
            ],
            "style": [
                "dist/style/index.d.ts"
            ],
            "theme": [
                "dist/theme/index.d.ts"
            ],
            "utils/object": [
                "dist/utils/object.d.ts"
            ],
            "utils/collection": [
                "dist/utils/collection.d.ts"
            ],
            "utils/dom": [
                "dist/utils/dom.d.ts"
            ],
            "utils/path": [
                "dist/utils/path.d.ts"
            ],
            "utils/number": [
                "dist/utils/number.d.ts"
            ]
        }
    },
    "scripts": {
        "build": "tsup",
        "build:watch": "tsup --watch",
        "test": "vitest"
    },
    "devDependencies": {
        "tsup": "^6.5.0",
        "typescript": "^4.9.4",
        "vitest": "^0.27.3"
    }
}
